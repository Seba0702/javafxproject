<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="com.mycompany.foodwaste_project.GameOverController">
   <children>
      <Label layoutX="232.0" layoutY="52.0" text="Gameover" textFill="RED">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label layoutX="257.0" layoutY="98.0" text="You lost">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="238.0" layoutY="164.0" text="You scored:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="pointLabel" layoutX="279.0" layoutY="207.0" text="Label">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="quitButton" layoutX="274.0" layoutY="292.0" mnemonicParsing="false" onAction="#quitGame" text="Quit" />
   </children>
</AnchorPane>
